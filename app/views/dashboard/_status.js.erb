<script>

$(document).ready(function() {

    // I just assumed this path
    var promise = $.getJSON('/status.json');

    // Render chart
    promise.done(function(data){
      Morris.Donut({
        colors: ['#5cb85c', '#f0ad4e', '#d9534f'],
        resize: true,
        element: 'status-donut-chart',
        data: data
      });
    });
});

</script>